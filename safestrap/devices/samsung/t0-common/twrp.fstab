# mount point   fstype          device

/external_sd  vfat      /dev/block/mmcblk1p1  /dev/block/mmcblk1            flags=display="Micro SDcard";storage;wipeingui;removable
/datamedia    ext4      /dev/block/mmcblk0p16-orig                          flags=display="Internal Storage";storage;backup=n
/data         ext4      /dev/block/mmcblk0p16
/cache        ext4      /dev/block/mmcblk0p12
/system       ext4      /dev/block/mmcblk0p13                               flags=usermrf
/systemorig   ext4      /dev/block/mmcblk0p13-orig                          flags=hidden;usermrf
/ss           ext4      /dev/block/mmcblk0p16-orig                          flags=hidden